@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix epp:   <http://lv2plug.in/ns/ext/port-props#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
<https://faustlv2.bitbucket.io/kpp_octaver>
       a lv2:Plugin ;
       doap:name "kpp_octaver" ;
       lv2:binary <kpp_octaver.so> ;
       lv2:optionalFeature epp:supportsStrictBounds ;
       lv2:optionalFeature lv2:hardRtCapable ;
       doap:maintainer [ foaf:name "Oleg Kapitonov" ] ;
       doap:revision "0.9b" ;
       doap:license "GPLv3" ;
    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "_9_bypass_0" ;
	lv2:name "99_bypass" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
	lv2:default 0.00000 ;
	lv2:minimum 0.00000 ;
	lv2:maximum 1.00000 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 1 ;
	lv2:symbol "cutoff_frequency_1" ;
	lv2:name "cutoff frequency" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 1000 ;
	lv2:default 160 ;
	lv2:minimum 100 ;
	lv2:maximum 200 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 2 ;
	lv2:symbol "dry_2" ;
	lv2:name "dry" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3000 ;
	lv2:default 30 ;
	lv2:minimum 0 ;
	lv2:maximum 30 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 3 ;
	lv2:symbol "octave1_3" ;
	lv2:name "octave1" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3000 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 30 ;
    ] , [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 4 ;
	lv2:symbol "octave2_4" ;
	lv2:name "octave2" ;
        lv2:portProperty epp:hasStrictBounds ;
        epp:rangeSteps 3000 ;
	lv2:default 0 ;
	lv2:minimum 0 ;
	lv2:maximum 30 ;
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 5 ;
	lv2:symbol "in0" ;
	lv2:name "in0" ;
    ] , [
	a lv2:InputPort ;
	a lv2:AudioPort ;
	lv2:index 6 ;
	lv2:symbol "in1" ;
	lv2:name "in1" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 7 ;
	lv2:symbol "out0" ;
	lv2:name "out0" ;
    ] , [
	a lv2:OutputPort ;
	a lv2:AudioPort ;
	lv2:index 8 ;
	lv2:symbol "out1" ;
	lv2:name "out1" ;
    ]
.
